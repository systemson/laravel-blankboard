{
    "name": "systemson/laravel-blankboard",
    "description": "Base project for laravel develops",
    "type": "library",
    "authors": [
        {
            "name": "Deivi Pe√±a",
            "email": "systemson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/systemson/laravel-model-validations"
        }
    ],
    "autoload": {
        "psr-4": {
            "Systemson\\Blankboard\\": "src/"
        }
    },
    "require": {
        "illuminate/support": "^7.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "systemson/laravel-model-validations": "^0.1.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "Systemson\\Blankboard\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "process-timeout": 0
    }
}
